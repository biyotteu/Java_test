package ¡Ÿ¿”∏ª;

import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		scan.nextLine();
		for(int i=0;i<n;i++) {
			String str = scan.nextLine();
			String[] arr = str.split(" ");
			for(int j=0;j<arr.length;j++) {
				System.out.print(arr[j].charAt(0));
			}
			System.out.println();
		}
	}
}
